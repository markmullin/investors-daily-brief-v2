  "scripts": {
    "start": "node src/index.js",
    "dev": "nodemon src/index.js",
    "health": "curl http://localhost:5000/health",
    "docs": "echo \"API documentation available at http://localhost:5000/api\"",
    "ai:server": "start-ai-server.bat",
    "ai:test": "node test-gpt-oss.js",
    "test:integration": "powershell -ExecutionPolicy Bypass -File test-integration.ps1",
    "dev:full": "echo Starting AI server and backend... && start cmd /k start-ai-server.bat && timeout /t 10 && npm run dev",
    "analysis:start": "start-accurate-analysis.bat",
    "analysis:test": "node test-intelligent-analysis-fixed.js",
    "test:analysis": "run-analysis-tests.bat",
    "diagnose": "node diagnose-analysis.js",
    "test:flow": "node test-data-flow.js",
    "fix:all": "echo Starting all fixes... && start cmd /k npm run analysis:start && timeout /t 5 && npm run dev"
  },
