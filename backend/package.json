{
  "name": "market-dashboard-backend",
  "version": "2.0.0",
  "type": "module",
  "main": "src/index.js",
  "description": "Production-grade market dashboard backend with authentication, rate limiting, and microservices architecture",
  "author": "Market Dashboard Team",
  "license": "MIT",
  "scripts": {
    "start": "node src/index.js",
    "dev": "node scripts/start-dev.js",
    "dev:backend": "nodemon src/index.js",
    "dev:python": "cd python && python -m uvicorn app.main:app --host 0.0.0.0 --port 8000 --reload",
    "build": "echo \"No build step needed for Node.js backend\"",
    "setup": "node scripts/complete-setup.js",
    "db:setup": "node scripts/setup-databases.js",
    "db:migrate": "node scripts/setup-databases.js",
    "python:install": "cd python && pip install -r requirements.txt",
    "python:start": "cd python && python -m uvicorn app.main:app --host 0.0.0.0 --port 8000 --reload",
    "test": "mocha tests/**/*.test.js --timeout 30000 --exit",
    "test:integration": "mocha tests/integration.test.js --timeout 30000 --exit",
    "test:risk-positioning": "node scripts/test-risk-positioning.js",
    "test:watch": "mocha tests/**/*.test.js --timeout 30000 --watch",
    "logs": "tail -f logs/combined.log",
    "logs:error": "tail -f logs/error.log",
    "logs:api": "tail -f logs/api.log",
    "cache:clear": "node scripts/clear-all-cache.js",
    "health": "curl http://localhost:5000/health",
    "docs": "echo \"API documentation available at http://localhost:5000/api\"",
    "lint": "echo \"Linting not configured yet\"",
    "format": "echo \"Formatting not configured yet\""
  },
  "dependencies": {
    "@mistralai/mistralai": "^1.7.2",
    "async": "^3.2.5",
    "axios": "^1.10.0",
    "bcryptjs": "^2.4.3",
    "chalk": "^5.4.1",
    "compression": "^1.8.0",
    "cors": "^2.8.5",
    "dotenv": "^16.4.1",
    "express": "^4.18.2",
    "express-rate-limit": "^7.1.5",
    "express-validator": "^7.0.1",
    "helmet": "^8.1.0",
    "http-proxy-middleware": "^2.0.6",
    "influx": "^5.9.3",
    "ioredis": "^5.3.2",
    "joi": "^17.11.0",
    "jsonwebtoken": "^9.0.2",
    "morgan": "^1.10.0",
    "node-cache": "^5.1.2",
    "node-cron": "^3.0.3",
    "pg": "^8.11.3",
    "pg-pool": "^3.6.1",
    "puppeteer": "^24.10.0",
    "uuid": "^9.0.1",
    "winston": "^3.11.0",
    "ws": "^8.18.0"
  },
  "devDependencies": {
    "chai": "^4.3.10",
    "jest": "^29.7.0",
    "mocha": "^10.2.0",
    "nodemon": "^3.0.3",
    "supertest": "^6.3.3"
  },
  "engines": {
    "node": ">=18.0.0",
    "npm": ">=8.0.0"
  },
  "keywords": [
    "market-dashboard",
    "financial-data",
    "portfolio-management",
    "stock-analysis",
    "investment-tools",
    "fintech",
    "api",
    "backend",
    "risk-positioning",
    "interactive-gauge"
  ],
  "repository": {
    "type": "git",
    "url": "git+https://github.com/yourusername/market-dashboard.git"
  },
  "bugs": {
    "url": "https://github.com/yourusername/market-dashboard/issues"
  },
  "homepage": "https://github.com/yourusername/market-dashboard#readme"
}
